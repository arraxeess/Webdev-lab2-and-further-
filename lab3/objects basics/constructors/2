<!DOCTYPE html>
<script>
"use strict";

function Accumulator(startingValue){
  this.value=startingValue;
  this.read=function(){
    let x=prompt("enter value:",0);
  }
}

let accumulator = new Accumulator(1); // initial value 1

accumulator.read(); 
accumulator.read(); 
alert(accumulator.value)
</script>
